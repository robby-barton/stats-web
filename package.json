{
	"private": true,
	"engine": {
		"npm": ">=10.0.0 <11.0.0",
		"node": ">=23.0.0 <24.0.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"test": "jest",
		"build:assets": "vite build",
		"build:11ty": "DEBUG=Eleventy* eleventy",
		"build:eleventy": "yarn build:assets && yarn build:11ty",
		"dev:assets": "vite build --watch",
		"dev:sync": "chokidar \"src/assets/build/**/*\" -c \"node -e \\\"require('fs').writeFileSync('src/_data/.assets-stamp', Date.now().toString())\\\"\"",
		"dev:eleventy": "concurrently -k \"yarn dev:assets\" \"yarn dev:sync\" \"yarn dev:11ty\"",
		"dev:11ty": "eleventy --serve",
		"docker:build": "DOCKER_BUILDKIT=1 docker build --target stats-web --tag stats-web:local --build-arg local=true .",
		"docker:run": "docker run -p 3000:3000 stats-web:local"
	},
	"dependencies": {
		"@amcharts/amcharts5": "^5.4.10",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@tanstack/react-table": "^8.10.6",
		"next": "^15.0.1",
		"postgres": "^3.3.4",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@eslint/compat": "^1.2.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.13.0",
		"@jest/globals": "^29.7.0",
		"@next/bundle-analyzer": "^15.0.1",
		"@swc/core": "^1.3.92",
		"@swc/jest": "^0.2.26",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.1.3",
		"@testing-library/react": "^16.0.1",
		"@types/jest": "^29.5.5",
		"@types/node": "22.7.8",
		"@types/react": "18.3.11",
		"@vitejs/plugin-react": "^4.3.1",
		"@typescript-eslint/eslint-plugin": "^8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^8.2.2",
		"eslint": "^9.13.0",
		"eslint-config-next": "^15.0.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^5.0.0",
		"dotenv": "^16.4.5",
		"globals": "^15.11.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"prettier": "^3.0.3",
		"ts-jest": "^29.1.0",
		"typescript": "5.6.3",
		"typescript-eslint-language-service": "^5.0.5",
		"vite": "^5.4.11"
	}
}
