{
  "private": true,
  "packageManager": "yarn@1.22.19",
  "engines": {
    "npm": ">=11.0.0 <12.0.0",
    "node": ">=24.13.0 <25.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "build:assets": "vite build",
    "build:11ty": "node scripts/eleventy-progress.mjs",
    "build:eleventy": "yarn build:assets && yarn build:11ty",
    "dev:assets": "vite build --watch",
    "dev:sync": "chokidar \"src/assets/build/**/*\" -c \"node -e \\\"require('fs').writeFileSync('src/_data/.assets-stamp', Date.now().toString())\\\"\"",
    "dev:eleventy": "concurrently -k \"yarn dev:assets\" \"yarn dev:sync\" \"yarn dev:11ty\"",
    "dev:11ty": "eleventy --serve",
    "dev": "yarn dev:eleventy",
    "build": "yarn build:eleventy",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.4.10",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-table": "^8.10.6",
    "postgres": "^3.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/node": "24.10.12",
    "@types/react": "18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^5.1.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.11.0",
    "prettier": "^3.0.3",
    "typescript": "5.6.3",
    "typescript-eslint-language-service": "^5.0.5",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
